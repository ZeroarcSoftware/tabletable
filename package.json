{
  "name": "tabletable",
  "version": "3.2.0",
  "description": "A simple and extremely flexible table component written in React.",
  "main": "dist/Container.js",
  "scripts": {
    "build": "NODE_ENV=production $(npm bin)/babel src/ -d dist/ && flow-copy-source -v src dist",
    "watch": "NODE_ENV=production $(npm bin)/babel --watch src/ -d dist/ && flow-copy-source -v src dist",
    "demo-watch": "$(npm bin)/webpack -w",
    "demo-build": "$(npm bin)/webpack",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeroarcSoftware/tabletable.git"
  },
  "keywords": [
    "Reactjs",
    "Table"
  ],
  "author": "Zeroarc Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZeroarcSoftware/tabletable/issues"
  },
  "homepage": "https://github.com/ZeroarcSoftware/tabletable#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-flow-strip-types": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-flow-react-proptypes": "^25.0.0",
    "flow-bin": "^0.94.0",
    "flow-copy-source": "^2.0.3",
    "flow-typed": "^2.4.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "classnames": "^2.2.5",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
