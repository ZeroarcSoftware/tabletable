    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/classnames": "^2.2.11",
    "autobind-decorator": "^2.4.0",
    "classnames": "^2.2.5",
    "immutable": "^4.0.0-rc.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
