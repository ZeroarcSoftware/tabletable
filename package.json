{
  "name": "tabletable",
  "version": "4.3.0",
  "description": "A simple and extremely flexible table component written in React.",
  "main": "dist/Container.js",
  "types": "dist/Container.d.ts",
  "scripts": {
    "build": "NODE_ENV=production $(npm bin)/babel src/ -d dist/ --extensions '.ts,.tsx' && tsc",
    "check-types": "tsc --noEmit",
    "watch": "NODE_ENV=production $(npm bin)/babel --watch src/ -d dist/ --extensions '.ts,.tsx' && tsc --watch",
    "demo-watch": "$(npm bin)/webpack -w",
    "demo-build": "$(npm bin)/webpack",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeroarcSoftware/tabletable.git"
  },
  "keywords": [
    "Reactjs",
    "Table"
  ],
  "author": "Zeroarc Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZeroarcSoftware/tabletable/issues"
  },
  "homepage": "https://github.com/ZeroarcSoftware/tabletable#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-loader": "^8.1.0",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.6",
    "autobind-decorator": "^2.4.0",
    "classnames": "^2.2.5",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
