{
  "name": "tabletable",
  "version": "2.0.1-beta",
  "description": "A simple and extremely flexible table component written in React.",
  "main": "dist/Container.js",
  "scripts": {
    "demo-build-old": "$(npm bin)/browserify --extension=.jsx -e src/demo.jsx -o test/index.js",
    "demo-watch-old": "$(npm bin)/watchify --extension=.jsx -e src/demo.jsx -o test/index.js",
    "build": "NODE_ENV=production $(npm bin)/babel src/ -d dist/ && flow-copy-source -v src dist",
    "watch": "NODE_ENV=production $(npm bin)/babel --watch src/ -d dist/ && flow-copy-source -v src dist",
    "demo-watch": "$(npm bin)/webpack -w",
    "demo-build": "$(npm bin)/webpack",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeroarcSoftware/tabletable.git"
  },
  "keywords": [
    "Reactjs",
    "Table"
  ],
  "author": "Zeroarc Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZeroarcSoftware/tabletable/issues"
  },
  "homepage": "https://github.com/ZeroarcSoftware/tabletable#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^9.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "flow-bin": "^0.59.0",
    "flow-copy-source": "^1.2.1",
    "flow-typed": "^2.2.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "webpack": "^3.8.1"
  },
  "peerDependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "react-addons-shallow-compare": "^15.6.2"
  }
}
