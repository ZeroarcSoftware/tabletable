{
  "name": "tabletable",
  "version": "5.0.2",
  "description": "A simple and extremely flexible table component written in React.",
  "main": "dist/Container.js",
  "types": "dist/Container.d.ts",
  "scripts": {
    "build": "NODE_ENV=production $(npm bin)/babel src/ -d dist/ --extensions '.ts,.tsx' && tsc",
    "check-types": "tsc --noEmit",
    "demo-build": "$(npm bin)/webpack",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "tsc",
    "watch": "NODE_ENV=production $(npm bin)/babel --watch src/ -d dist/ --extensions '.ts,.tsx'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeroarcSoftware/tabletable.git"
  },
  "keywords": [
    "Reactjs",
    "Table"
  ],
  "author": "Zeroarc Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZeroarcSoftware/tabletable/issues"
  },
  "homepage": "https://github.com/ZeroarcSoftware/tabletable#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "babel-loader": "^8.2.2",
    "react": "^18.1.0",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "classnames": "^2.3.1",
    "immutable": "^4.0.0-rc.14"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  }
}
